# Neynar API key (required for search, notifications, user lookups)
NEYNAR_API_KEY=your-neynar-api-key

# Your Farcaster FID (optional if set in signer JSON)
FARCASTER_FID=1103255

# Path to signer key JSON (default: ~/.marvin/secrets/farcaster-signer.json)
SIGNER_KEY_PATH=/path/to/farcaster-signer.json

# State file path for dedup/rate limiting (default: .farcaster-state.json)
FARCASTER_STATE_PATH=.farcaster-state.json
